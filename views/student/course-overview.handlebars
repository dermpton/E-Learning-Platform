{{!-- Fetch what the user may be registered for in their course selection  --}}


{{!-- {{#each course }} --}}
    <div class="organize-course-template">
    <div class="course-template">
        <a href="/student/course/:course_code"><h6>{{ courseName}}</h6></a>
        <p>Taken by {{ teachersName }}</p>
        <form action="#" method="get">
            <label for="leave"></label>
            <input type="button" name="leave" value="Leave" class="leave-btn">
        </form>   
    </div>
</div>
{{!-- {{/each }} --}}

{{!-- Use the each to generate multiple instances --}}